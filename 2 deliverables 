      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "from scipy.integrate import odeint\n",
        "\n",
        "# Assumed physical parameters (replace with actual when available)\n",
        "Iz = 0.25  # Moment of inertia in kg·m²\n",
        "d = 0.13   # Distance from center to motor in meters\n",
        "\n",
        "# PID controller gains\n",
        "Kp = 1.5\n",
        "Ki = 0.1\n",
        "Kd = 0.2\n",
        "\n",
        "# Desired yaw angle in radians (45 degrees)\n",
        "theta_desired = np.pi / 4\n",
        "\n",
        "# Time vector\n",
        "t = np.linspace(0, 10, 1000)\n",
        "dt = t[1] - t[0]\n",
        "\n",
        "# System dynamics with embedded PID controller\n",
        "# State: [theta, omega, error_sum, prev_error]\n",
        "def yaw_dynamics_fixed(state, t):\n",
        "    theta, omega, error_sum, prev_error = state\n",
        "    error = theta_desired - theta\n",
        "    error_sum += error * dt\n",
        "    d_error = (error - prev_error) / dt\n",
        "    torque = d * (Kp * error + Ki * error_sum + Kd * d_error)\n",
        "    dtheta_dt = omega\n",
        "    domega_dt = torque / Iz\n",
        "    return [dtheta_dt, domega_dt, error_sum, error]\n",
        "\n",
        "# Initial conditions\n",
        "init_state_fixed = [0, 0, 0, 0]  # [theta, omega, error_sum, prev_error]\n",
        "\n",
        "# Solve the system\n",
        "solution_fixed = odeint(yaw_dynamics_fixed, init_state_fixed, t)\n",
        "\n",
        "# Extract yaw angle\n",
        "theta_fixed = solution_fixed[:, 0]\n",
        "\n",
        "# Plot the results\n",
        "plt.figure(figsize=(10, 5))\n",
        "plt.plot(t, theta_fixed, label='Yaw Angle (rad)', linewidth=2)\n",
        "plt.axhline(theta_desired, color='r', linestyle='--', label='Desired Yaw (rad)')\n",
        "plt.title('Corrected PID-Controlled Yaw Angle vs Time')\n",
        "plt.xlabel('Time (s)')\n",
        "plt.ylabel('Yaw Angle (rad)')\n",
        "plt.legend()\n",
        "plt.grid(True)\n",
        "plt.tight_layout()\n",
        "plt.show()\n"
      ]
    }
  ]
}